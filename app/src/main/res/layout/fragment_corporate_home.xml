<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.sticker_android.controller.fragment.corporate.CorporateHomeFragment">
   <!-- <RelativeLayout
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <include layout="@layout/tab_with_divider"/>
    </RelativeLayout>
-->
    <include  android:id="@+id/tabLayout" layout="@layout/tab_layout_without_viewpager"/>

    <RelativeLayout
        android:layout_below="@+id/tabLayout"
        android:id="@+id/container_contest"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></RelativeLayout>


    <android.support.design.widget.FloatingActionButton
        app:backgroundTint="@color/colorFloatingCorporate"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:id="@+id/fabAddNew"
        android:elevation="6dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_add"
        android:layout_margin="@dimen/add_corp_margin" />
</RelativeLayout>
